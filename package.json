{
  "name": "@fireflysemantics/validator",
  "version": "1.0.0",
  "description": "Typescript Validation Decorators and Executor",
  "main": "index.ts",
  "scripts": {
    "clean": "del-cli target && del-cli dist && del-cli doc",
    "unit-test": "mocha -r ts-node/register src/**/*.spec.ts",
    "test": "nyc mocha",
    "doc": "typedoc --out doc --exclude **/*.spec.ts ./src/",
    "compile": "del-cli target && tsc",
    "dist": "npm run compile && del-cli dist && node createdist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fireflysemantics/validator.git"
  },
  "keywords": [
    "Typescript",
    "Decorators",
    "Validation",
    "Validator"
  ],
  "_moduleAliases": {
    "@fireflysemantics/*": ".target/src/*"
  },
  "author": "Ole Ersoy",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fireflysemantics/validator/issues"
  },
  "homepage": "https://github.com/fireflysemantics/validator#readme",
  "devDependencies": {
    "@types/chai": "^4.1.4",
    "@types/mocha": "^5.2.2",
    "@types/node": "^10.3.4",
    "chai": "^4.1.2",
    "cpy": "^7.0.1",
    "del-cli": "^1.1.0",
    "globby": "^8.0.1",
    "istanbul": "^0.4.5",
    "mkdirp": "^0.5.1",
    "mocha": "^5.2.0",
    "nyc": "^12.0.2",
    "recursive-copy": "^2.0.9",
    "source-map-support": "^0.5.6",
    "ts-node": "^6.1.2",
    "tsconfig-paths": "^3.4.0",
    "typedoc": "^0.11.1",
    "typescript": "^2.9.2"
  },
  "dependencies": {
    "@fireflysemantics/is": "^1.8.0",
    "@types/validator": "^9.4.1",
    "typescript-logging": "^0.6.2",
    "validator": "^10.4.0"
  }
}
