{
  "name": "@fireflysemantics/validator",
  "version": "1.0.10",
  "description": "Typescript Validation Decorators and Executor",
  "main": "index.ts",
  "scripts": {
    "clean": "del-cli target && del-cli dist && del-cli doc",
    "test": "jest",
    "doc": "typedoc --out doc --exclude **/*.spec.ts ./src/",
    "compile": "del-cli target && tsc",
    "dist": "npm run compile && del-cli dist && node createdist",
    "update": "ncu -a && npm i"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fireflysemantics/validator.git"
  },
  "keywords": [
    "Typescript",
    "Decorators",
    "Validation",
    "Validator"
  ],
  "author": "Ole Ersoy",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fireflysemantics/validator/issues"
  },
  "homepage": "https://github.com/fireflysemantics/validator#readme",
  "devDependencies": {
    "@types/jest": "^23.3.5",
    "@types/node": "^10.12.0",
    "cpy": "^7.0.1",
    "del-cli": "^1.1.0",
    "globby": "^8.0.1",
    "jest": "^23.6.0",
    "mkdirp": "^0.5.1",
    "npm-check-updates": "^2.14.2",
    "recursive-copy": "^2.0.9",
    "source-map-support": "^0.5.9",
    "ts-jest": "^23.10.4",
    "ts-node": "^7.0.1",
    "tsconfig-paths": "^3.6.0",
    "typedoc": "^0.13.0",
    "typescript": "^3.1.3"
  },
  "dependencies": {
    "@fireflysemantics/is": "^1.8.0",
    "@types/validator": "^9.4.2",
    "typescript-logging": "^0.6.3",
    "validator": "^10.8.0"
  }
}
