{
  "name": "@fireflysemantics/validator",
  "version": "2.0.1",
  "description": "Typescript Validation Decorators and Executor",
  "main": "target/index.js",
  "types": "target/index.d.ts",
  "scripts": {
    "test": "jest",
    "w": "del-cli target && del-cli doc",
    "d": "typedoc --out doc --exclude **/*.spec.ts ./src/",
    "c": "del-cli target && tsc",
    "p": "bump patch && npm run c && npm publish",
    "u": "ncu -u && npm i && npm-install-peers"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fireflysemantics/validator.git"
  },
  "keywords": [
    "Typescript",
    "Decorators",
    "Validation",
    "Validator"
  ],
  "author": "Ole Ersoy",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fireflysemantics/validator/issues"
  },
  "homepage": "https://github.com/fireflysemantics/validator/#readme",
  "devDependencies": {
    "@types/jest": "^25.1.4",
    "@types/node": "^13.9.0",
    "cpy": "^8.1.0",
    "del-cli": "^3.0.0",
    "globby": "^11.0.0",
    "jest": "^25.1.0",
    "mkdirp": "^1.0.3",
    "recursive-copy": "^2.0.10",
    "source-map-support": "^0.5.16",
    "ts-jest": "^25.2.1",
    "ts-node": "^8.6.2",
    "tsconfig-paths": "^3.9.0",
    "typedoc": "^0.16.11",
    "typescript": "^3.8.3"
  },
  "peerDependencies": {
    "@fireflysemantics/is": "*"
  },
  "comments": {
    "global dependencies": {
      "@jsdevtools/version-bump-prompt global install": "npm i -g @jsdevtools/version-bump-prompt",
      "npm-check-updates": "npm i -g npm-check-updates",
      "npm-install-peers": "npm i -g npm-install-peers"
    }
  }
}
